<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.demo.OBS.Model.Fields" table="fields" schema="obs">
        <id name="id">
            <column name="id" sql-type="int(10)"/>
        </id>
        <property name="centerId">
            <column name="center_id" sql-type="int(10)"/>
        </property>
        <property name="size">
            <column name="size" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="fieldType">
            <column name="field_type" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>