<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.demo.OBS.Model.Location" table="location" schema="obs">
        <id name="id">
            <column name="id" sql-type="int(10)"/>
        </id>
        <property name="x">
            <column name="x" sql-type="float" precision="-1"/>
        </property>
        <property name="y">
            <column name="y" sql-type="float" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>